  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier . --check",
    "format": "prettier . --write",
    "preview": "vite preview --port=5173",
    "sizecheck": "npx vite-bundle-visualizer",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "schema:fetch": "curl 'http://localhost:8080/v3/api-docs' -o openapi.json",
    "schema:format": "prettier --write openapi.json",
    "schema:generate": "npm run schema:fetch && npm run schema:format && openapi-typescript ./openapi.json -o src/generated/api-schema.d.ts"
  },
